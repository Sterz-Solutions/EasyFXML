<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="moe.tristan.easyfxml.samples.form.user.view.userform.fields.email.EmailController">
    <children>
        <Label text="Label">
            <font>
                <Font name="System Italic" size="10.0"/>
            </font>
        </Label>
        <TextField fx:id="emailField" promptText="john.doe@mail.com"/>
        <Label visible="false" fx:id="errorLabel" text="Label" textFill="RED">
            <font>
                <Font size="10.0"/>
            </font>
        </Label>
    </children>
</VBox>
